    "disk.readRate": {
        "displayName": "Disk Read Rate",
        "units": "MB/s",
        "max": null,
        "min": 0,
        "sigFigs": null,
        "dataType": "numeric",
        "type": "derived",
        "formula": {
            "equation": "\\frac{sector_{read}*sector_{size}}{1000*t_{reading}}",
            "variables": {
                "sector_{read}": {
                    "description": "the total number of sectors read on an individual disk since the last reboot"
                },
                "sector_{size}": {
                    "description": "the size of a logical sector on an individual disk"
                },
                "t_{reading}": {
                    "description": "the amount of time an individual disk spent reading since the last reboot"
                }
            }
        },
        "description": "The amount of data read divided by the time spent reading on an individual disk since the last reboot.",
        "notes": "",
