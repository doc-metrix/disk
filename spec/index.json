    "disk.spaceUtilization": {
        "display_name": "Disk Space Utilization",
        "units": "utilization",
        "max": 1,
        "min": 0,
        "sig_figs": null,
        "data_type": "percentage",
        "type": "derived",
        "formula": {
            "equation": "\\frac{disk_{used}}{disk_{size}}",
            "variables": {
                "disk_{used}": {
                    "description": "The amount of space used on this disk.",
                    "reference": "https://github.com/doc-metrix/disk"
                },
                "disk_{size}": {
                    "description": "The total amount of space on this disk.",
                    "reference": "https://github.com/doc-metrix/disk"
                }
            }
        },
        "description": "The disk space utilization (used storage divided by total size) for this disk.",
        "notes": "This metric is only calculated for disk drive names starting with 'sd' in the NMA code. The NMA code is dividing by available storage.",
        "platforms": null,
        "device": "sd",
        "vendors": "*",
        "classification": "disk",
        "refs": [
            "http://linux.die.net/man/1/df",
            "https://www.kernel.org/doc/Documentation/devices.txt"
        ]
    },
    "disk.meanSpaceUtilization": {
        "display_name": "Mean Disk Space Utilization",
        "units": "utilization",
        "max": 1,
        "min": 0,
        "sig_figs": null,
        "data_type": "percentage",
        "type": "derived",
        "formula": {
            "equation": "\\frac{\\sum_{disks}\\frac{disk_{used}}{disk_{size}}}{n_{disks}}",
            "variables": {
                "disk_{used}": {
                    "description": "The amount of space used on this disk.",
                    "reference": "https://github.com/doc-metrix/disk"
                },
                "disk_{size}": {
                    "description": "The total amount of space on this disk.",
                    "reference": "https://github.com/doc-metrix/disk"
                },
                "n_{disks}": {
                    "description": "The total number of disks.",
                    "reference": "https://github.com/doc-metrix/disk"
                }
            }
        },
        "description": "The mean disk space utilization (used storage divided by total size) across all disks.",
        "notes": "This metric is only calculated for disk drive names starting with 'sd' in the NMA code. The NMA code is dividing by available storage.",
        "platforms": null,
        "device": null,
        "vendors": "*",
        "classification": "disk",
        "refs": [
            "http://linux.die.net/man/1/df",
            "https://www.kernel.org/doc/Documentation/devices.txt"
        ]
    },
    "disk.timeUtilization": {
        "display_name": "Disk Time Utilization",
        "units": "utilization",
        "max": 1,
        "min": 0,
        "sig_figs": null,
        "data_type": "percentage",
        "type": "derived",
        "formula": {
            "equation": "\\frac{time_{disk in use}}{time_{uptime}}",
            "variables": {
                "time_{disk in use}": {
                    "description": "The amount of time the disk has spent doing IO since the last reboot.",
                    "reference": "https://github.com/doc-metrix/disk"
                },
                "time_{uptime}": {
                    "description": "The total amount of time the system has been running since the last reboot.",
                    "reference": "https://github.com/doc-metrix/disk"
                }
            }
        },
        "description": "The disk time utilization (time spent doing IO divided by system uptime) for this disk.",
        "notes": "",
        "platforms": null,
        "device": "sd",
        "vendors": "*",
        "classification": "disk",
        "refs": [
            "http://linux.die.net/man/1/df",
            "https://www.kernel.org/doc/Documentation/devices.txt"
        ]
    },
